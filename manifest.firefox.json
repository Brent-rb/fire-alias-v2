{
	"manifest_version": 2,
	"name": "FireAlias",
	"version": "1.1",
	"description": "Create aliases for websites.",
	"icons": {
		"16": "icon.png",
		"24": "icon.png",
		"32": "icon.png",
		"48": "icon.png"
	},
	"omnibox": {
		"keyword": "goto"
	},
	"browser_action": {
		"default_icon": {
			"16": "icon.png",
			"24": "icon.png",
			"32": "icon.png",
			"48": "icon.png"
		},
		"default_title": "FireAlias",
		"default_popup": "index.html"
	},
	"permissions": ["storage"],
	"background": {
		"scripts": ["./background.js"]
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "firealias@brent-rb.github",
			"strict_min_version": "58.0"
		}
	}
}
